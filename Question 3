Task_1
def update_dictionary(dct, key, value):
    if key in dct:
        print(f"Original value for '{key}': {dct[key]}")
    dct[key] = value
    return dct

Task_2
#Scenario1
result1 = update_dictionary({}, "name", "Alice")
print("Result 1:", result1)
#Scenario2
result2 = update_dictionary({"age": 25}, "age", 26)
print("Result 2:", result2)
